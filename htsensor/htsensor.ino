RHT03 rht; // This creates a RTH03 object, which we'll use to interact with the sensor
void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600); // Serial is used to print sensor readings.
  //sayNumber(1113.0);
  //for(int i = 0;i<1000;i++)
  //{
  //  sayNumber((float)i);
  //}
display.begin(SSD1306_SWITCHCAPVCC);



  display.display();
  delay(2000);
  //setupUI();
  display.display();
  
  
  // Call rht.begin() to initialize the sensor and our data pin
  rht.begin(RHT03_DATA_PIN);
//  voice.say(sp_thermokrasia);
}

void loop() {
  // put your main code here, to run repeatedly:
    int updateRet = rht.update();
  
  // If successful, the update() function will return 1.
  // If update fails, it will return a value <0
  if (updateRet == 1)
  {
    // The humidity(), tempC(), and tempF() functions can be called -- after 
    // a successful update() -- to get the last humidity and temperature
    // value 
    float latestHumidity = rht.humidity();
    float latestTempC = rht.tempC();
    float latestTempF = rht.tempF();
    
    // Now print the values:
    String s1 = "Humidity: "+String(latestHumidity, 1)+ " %";
    String s2 = "Temp (F): " + String(latestTempF, 1) + " F";
    String s3 = "Temp (C): " + String(latestTempC, 1) + " C";
    Serial.println(s1);
    Serial.println(s2);
    Serial.println(s3);
    s1.toCharArray(&(display.charbuffer[0]),16);
    s2.toCharArray(&(display.charbuffer[16]),16);
    s3.toCharArray(&(display.charbuffer[32]),16);
    voice.say(sp_thermokrasia);
    sayNumber(latestTempC);
    voice.say(sp_kelsiou);
    voice.say(sp_ygrasia);
    sayNumber(latestHumidity);
    voice.say(sp_toisekato);    
    
  }
  else
  {
    // If the update failed, try delaying for RHT_READ_INTERVAL_MS ms before
    // trying again.
    delay(RHT_READ_INTERVAL_MS);
  }
   display.begin_display();
   
       display.display_row(0);
       display.display_row(1);
       display.display_row(2);
       display.display_row(3);
       display.display_row(4);
       display.display_row(5);
       display.display_row(6);
       display.display_row(7);
       display.end_display();

}
void sayNumber(float num)
{
  int inum = (int)num;
  int dnum = (int)(10*(num - (float)inum));
  if(num ==0)
  {
    voice.say(sp_miden);
  }
  int prosimo = 1;
  if(num<0.0)
  {
    prosimo = -1;
    num = -num;
  }
  int disekatommyria = (int)(num / 1000000000.0);
  int ekatommyria = (int)(num - disekatommyria* 1000000000)/1000000.0;
  int xiliades = (int) (num - disekatommyria * 1000000000 - ekatommyria * 1000000) / 1000.0;
  int ekatondades = (int) (num - disekatommyria * 1000000000 - ekatommyria * 1000000 - xiliades*1000);
  //int dekades = (int) (num - disekatommyria * 1000000000 - ekatommyria * 1000000 - xiliades*1000 - 100 * ekatondades) / 10.0;
  Serial.println(num);
  if(prosimo==-1)
  {
    voice.say(sp_plin);
  }
  if(disekatommyria>0)
  {
    sayThreeDigits(disekatommyria);
    if(disekatommyria == 1)
    {
      voice.say(sp_disekatommyrio);
    }
    else
    {
      voice.say(sp_disekatommyria);
    }
  }
  if(ekatommyria>0)
  {
    sayThreeDigits(ekatommyria);
    if(ekatommyria == 1)
    {
      voice.say(sp_ekatommyria);
    }
    else
    {
      voice.say(sp_ekatommyria);
    }
  }
  if(xiliades>0)
  {
    
    if(xiliades == 1)
    {
      voice.say(sp_xilia);
    }
    else
    {
      sayThreeDigits(xiliades);
      voice.say(sp_xiliades);
    }
  }
  sayThreeDigits(ekatondades);
  if(dnum!=0)
  {
    voice.say(sp_comma);
    sayThreeDigits(dnum);
  }
}

